                            color: app.colors.primary
                            font_name: app.fonts.subheading
                            font_size: app.fonts.size.h4
                            halign: 'center'
        BoxLayout:
            spacing: dp(8)
            size_hint_y: .8
            orientation: 'vertical'
            ShadowBox:
