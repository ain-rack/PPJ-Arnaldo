<Home>:
    padding: dp(14)
    spacing: dp(16)
    BoxLayout:
        spacing: dp(16)
        orientation: 'vertical'
        size_hint_x: .80
        BoxLayout:
            spacing: dp(16)
            size_hint_y: .2
            ShadowBox:
                radius: [dp(14)]
                elevation: 2
                padding: dp(14)
                orientation: 'vertical'
                spacing: dp(8)
                BoxLayout:
                    spacing: dp(14)
                    AnchorLayout:
                        size_hint_x: None
                        width: self.height
                        IconButton:
                            source: 'assets\icons\ic_trend_active.png'           
                    BoxLayout:
                        orientation: 'vertical'
                        spacing: dp(8)
                        Text:
                            text: 'Venda Mensal'
                            font_name: app.fonts.body
                            font_size: app.fonts.size.h2
                            color: app.colors.secondary
                        Text:
                            text: root.vendas
                            font_name: app.fonts.heading
                            font_size: app.fonts.size.h1
                            color: app.colors.secondary
                BoxLayout:
                    size_hint_y: None
                    height: dp(24)
                    spacing: dp(8)
                    Text:
                        text: 'Comparação ao Mes Passado'
                        color: app.colors.secondary
                        font_name: app.fonts.body
                        font_size: app.fonts.size.h5
                        size_hint_x: .8
                    BoxLayout:
                        size_hint_x: .2
                        padding: [dp(4), dp(2)]
                        canvas.before:
                            Color:
                                rgba: rgba('#E1F1FF')
                            RoundedRectangle:
                                pos: self.pos
                                size: self.size
                                radius: [self.height/2]
                        Text:
                            text: '+8.5%'                                           #FAZER FUNCIONAR DEPOIS
                            color: app.colors.primary
                            font_name: app.fonts.subheading
                            font_size: app.fonts.size.h5
                            halign: 'center'
            ShadowBox:                                                              #TOTAL KITS
                radius: [dp(14)]
                elevation: 2
                padding: dp(14)
                orientation: 'vertical'
                spacing: dp(8)         
                BoxLayout:
                    spacing: dp(14)    
                    BoxLayout:
                        orientation: 'vertical'
                        spacing: dp(8)
                        Text:
                            text: 'Vendas Totais'
                            font_name: app.fonts.body
                            font_size: app.fonts.size.h1
                            color: app.colors.secondary
                            halign: 'center'
                        Text:
                            text: root.vendas2
                            font_name: app.fonts.heading
                            font_size: app.fonts.size.h1
                            color: app.colors.secondary
                            halign: 'center'
                BoxLayout:
                    size_hint_y: None
                    height: dp(24)
                    spacing: dp(8)
                    Text:
                        text: 'Renda Mensal Tirando 35%'
                        color: app.colors.secondary
                        font_name: app.fonts.body
                        font_size: app.fonts.size.h4
                        size_hint_x: 1
                        halign: 'center'
            ShadowBox:                                                              #TOTAL KITS
                radius: [dp(14)]
                elevation: 2
                padding: dp(14)
                orientation: 'vertical'
                spacing: dp(8)         
